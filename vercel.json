{
   "version": 2,
   "builds": [
      {
         "src": "./index.js",
         "use": "@vercel/node"
      }
   ],
   "routes": [
      { "handle": "filesystem" },
      {
         "src": "/(.*)",
         "dest": "index.js",
         "methodS": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      }
   ]
}
